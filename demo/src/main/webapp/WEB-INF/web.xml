<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>demo1</servlet-name>
        <servlet-class>com.gzpanfuyi.web.LoginCheck</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>demo1</servlet-name>
        <url-pattern>/login.action</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>getCertifacationCode</servlet-name>
        <servlet-class>com.gzpanfuyi.web.CertificationCode</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>getCertifacationCode</servlet-name>
        <url-pattern>/getCertifationCode</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>query</servlet-name>
        <servlet-class>com.gzpanfuyi.dao.QueryStudent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>query</servlet-name>
        <url-pattern>/query.action</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
